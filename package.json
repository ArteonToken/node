{
  "name": "@artonline/node",
  "productName": "ArtOnline Node",
  "setupName": "ArtOnlineNodeSetup",
  "version": "1.1.3",
  "description": "ArtOnline Node GUI",
  "company": "Artonline",
  "appId": "org.artonline.nodegui",
  "copyright": "Copyright Â© 2022 ArtOnline",
  "main": "./app/index.js",
  "scripts": {
    "install-dependencies": "npm i && cd app && npm i && cd ..",
    "upgrade-dependencies": "npm upgrade && cd app && npm upgrade && cd ..",
    "copy-contracts": "cp node_modules/@leofcoin/chain/src/contracts app/templates -r",
    "copy-standards": "cp node_modules/@leofcoin/chain/src/standards app/templates -r",
    "copy-templates": "npm run copy-standards && npm run copy-contracts",
    "docs": "sh node_modules/.bin/jsdoc --configure .jsdoc.json --verbose",
    "start": "cd app && electron index.js",
    "c": "rollup -c",
    "build:all": "rollup -c rollup.config.mjs && rollup -c rollup.monaco.config.mjs",
    "build": "npm run build:all && electron-kit",
    "build:app": "rollup -c rollup.config.mjs",
    "build:monaco": "rollup -c rollup.monaco.config.mjs",
    "watch": "rollup -c -w",
    "pack": "webpack",
    "start-dev": "npm run install-dependencies && npm run build:all && npm run pack && npm run start",
    "dev": "npm run install dependencies && npm run watch"
  },
  "keywords": [],
  "author": {
    "name": "vandeurenglenn",
    "email": "vandeurenglenn@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^10.0.1",
    "@vandeurenglenn/electron-kit": "^0.3.1",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.7.2",
    "electron-squirrel-startup": "^1.0.0",
    "file-loader": "^6.2.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "postcss-url": "^10.1.3",
    "rollup": "^3.12.0",
    "rollup-plugin-modify": "^3.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@leofcoin/chain": "^1.4.79",
    "@leofcoin/lib": "^1.0.6",
    "@leofcoin/peernet": "^0.15.2",
    "@leofcoin/peernet-swarm": "^0.4.2",
    "@leofcoin/utils": "^1.0.4",
    "@leofcoin/workers": "^1.3.1",
    "@vandeurenglenn/debug": "^1.0.0",
    "@vandeurenglenn/easy-worker": "^1.0.2",
    "@vandeurenglenn/flex-elements": "^0.1.0",
    "@vandeurenglenn/little-pubsub": "^1.3.3",
    "custom-pages": "^0.12.1",
    "custom-selector": "^0.2.1",
    "custom-svg-icon": "^0.10.2",
    "custom-svg-iconset": "^0.10.1",
    "electron": "^25.0.0-alpha.4",
    "javascript-time-ago": "^2.5.9",
    "lit": "^2.4.1",
    "monaco-editor": "^0.34.0",
    "ora": "^5.0.0",
    "vm-browserify": "^1.1.2"
  }
}
